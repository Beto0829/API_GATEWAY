{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "notas-route": {
        "ClusterId": "notas-cluster",
        "Match": {
          "Path": "Notas.Server/Dockerfile/{**catch-all}"
        },
        "Transforms": {
          "PathPattern": "{**catch-all}"
        }
      },
      "usuarios-route": {
        "ClusterId": "usuarios-cluster",
        "Match": {
          "Path": "Usuarios.Server/Dockerfile/{**catch-all}"
        },
        "Transforms": {
          "PathPattern": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "notas-cluster": {
        "Destination1": {
          "Address": "http://Notas.Server/Dockerfile:5081"
        },
        "usuarios-cluster": {
          "Destination2": {
            "Address": "http://Usuarios.Server/Dockerfile:5027"
          }
        }
      }
  }
}
